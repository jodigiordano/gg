{
  "name": "@gg",
  "version": "0.1.0",
  "type": "module",
  "license": "See LICENSE.md",
  "scripts": {
    "compile": "npm run compile --workspaces",
    "lint": "npm run lint --workspaces",
    "maintenance": "npm update --workspaces",
    "dev": "concurrently --kill-others --names core,app,server \"(cd core && npm run dev)\" \"(cd app && npm run dev)\" \"(cd server && npm run dev)\""
  },
  "devDependencies": {
    "concurrently": "8.x"
  },
  "workspaces": [
    "icons",
    "core",
    "app",
    "server",
    "infra"
  ]
}
